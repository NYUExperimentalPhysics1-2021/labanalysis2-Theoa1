        "import numpy as np\n",
        "\n",
        "T = 2\n",
        "A = 0.1\n",
        "m = 0.549\n",
        "p = np.pi \n",
        "\n",
        "E = (1.0/2.0)*m*((0.1*(2*np.pi)/T)**2)\n",
        "Eten = (f'{E:.1}')\n",
        "\n",
        "A= 0.01\n",
        "E = (1.0/2.0)*m*((A*(2*np.pi)/T)**2)\n",
        "Eone = (f'{E: .1}')\n",
        "print('1) The total Energy when A is 10cm is ', Eten)\n",
